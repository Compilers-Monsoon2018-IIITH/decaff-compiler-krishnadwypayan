// Factorial of a number : fact.dcf

class Program {

	// This method will return the factorial of a number.
	int factorial (int n) {
		int res , i ;
		res = 1 ;

		for i = 1 , n {
			res = res * i ;
			i += 1 ;
		}

		return res ;
	}
	
	// Execution of a Decaf program starts at method main.
	int main () {
		int n , ans ;
		n = 10 ;
		ans = factorial ( n ) ;
		callout ( "printf" , "Factorial of %d is %d" , n , ans ) ;
		return 0 ;
	}

}